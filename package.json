    "react-confetti": "^6.0.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.2.0",
    "@types/node": "^16.11.19",
